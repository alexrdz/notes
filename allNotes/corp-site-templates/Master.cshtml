@using Equitrust.Corp.Models;
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ISeo>
@using ClientDependency.Core.Mvc;
@{
    Layout = null;

    // CSS
    Html.RequiresCss("~/css/libs/bootstrap.min.css", 1);
    Html.RequiresCss("~/css/libs/font-awesome.min.css", 2);
    Html.RequiresCss("~/css/custom.css");

    // Scripts
    Html.RequiresJs("https://cdn.polyfill.io/v2/polyfill.min.js", 1, 1);
    Html.RequiresJs("~/scripts/libs/modernizr-2.8.3.min.js", 2, 1);
    Html.RequiresJs("~/scripts/libs/jquery-3.2.1.min.js", 3, 2);
    Html.RequiresJs("~/scripts/libs/jquery.placeholder.js", 4, 2);
    Html.RequiresJs("~/scripts/libs/popper.min.js", 5, 2);
    Html.RequiresJs("~/scripts/libs/bootstrap.min.js", 6, 2);
    Html.RequiresJs("~/scripts/libs/Chart.bundle.min.js", 7, 2);
    Html.RequiresJs("~/scripts/main.js", 8, 2);
    Html.RequiresJs("~/scripts/analytics.js", 9, 2);

    var siteSettings = Model.Content.AncestorOrSelf<ISiteSettings>();
}

<!DOCTYPE html>

<!-- The  no-js  class is provided in order to allow us to more easily and explicitly add custom styles based on whether JavaScript is disabled -->
<html class="no-js" lang="en" xmlns="http://www.w3.org/1999/html">

<head>
    @Html.Partial("SEO", Model.Content)
    @Html.RenderCssHere()
    <!--[if IE]>
    <link rel="stylesheet" href="~/css/custom-MS.css">
    <![endif]-->
    <!-- All of our HTML imports * THIS FEATURE SHOULD BE USED ONCE ALL BROWSERS SUPPORT HTML5 IMPORTS NATIVELY * -->
    <!--<link rel="import" href="index-account-detail">-->
</head>

<body class="BodyL BodyS visible-print">
    <!--[if lte IE 10]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade
    your browser</a> to greatly improve your experience. This site looks and performs best on the latest stable versions of Chrome, Safari, Firefox, or Microsoft Edge.
    </p>
    <![endif]-->
    <!-- MAIN HEADER FOR PRIMARY & SECONDARY NAVIGATION, AS WELL AS EQUITRUST HERO IMAGE -->
    <style>
    @media (min-width: 576px) {
      .site-header {
        @if (Model.Content is IHeroImageContainer)
        {
            var heroImage = (Model.Content as IHeroImageContainer).GetPropertyValue<Image>("imagePicker", true);
            if (heroImage != null)
            {
                background-image: url('@heroImage.Url');
            }
        }

        @if (siteSettings != null)
        {
            background - image: url('@siteSettings.SiteLogo.Url');
        }

        
      }
    }
    </style>

    <header class="container-fluid px-0 pt-16 h-sm-400 bg-cover bg-center bg-no-repeat site-header">


        <!-- FIRST NAVBAR -->
        @Html.Partial("first-navbar")
        @Html.Partial("hamburger-modal")

        <!-- SECOND NAVBAR (ONLY VISIBLE ON TABLETS AND LARGER) -->
        <!-- NAVBAR TOGGLER -->
        <nav class="row no-gutters second-navbar d-none no-print">
            <div class="col">
                <!--Source: /Views/Partials/second-navbar.cshtml-->
                <!-- ABOUT SUB-NAV -->
                <nav class="navbar navbar-light p-0 no-print" id="about_subnav" role="navigation" aria-expanded="false">
                    <ul class="navbar-nav d-flex flex-row justify-content-center align-items-center mx-auto" role="navigation" aria-expanded="false">
                        <li class="nav-item">
                            <a class="nav-link" id="companylink" href="/about/company/">
                                Company
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="communitylink" href="/about/community/">
                                Community
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="valueslink" href="/about/values/">
                                Values
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" id="leadershiplink" href="/about/leadership/">
                                Leadership
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                    </ul>
                </nav>

                <!-- CAREERS SUB-NAV -->
                <nav class="navbar navbar-light bg-color-heisenberg-80 p-0 display-none no-print" id="careers_subnav" role="navigation"
