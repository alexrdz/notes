                    <ul class="navbar-nav d-flex flex-row justify-content-center align-items-center mx-auto" role="navigation" aria-expanded="false">
                        <li class="nav-item TopNavL">
                            <a class="nav-link" id="culturelink" href="/careers/culture/">
                                Culture
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item TopNavL">
                            <a class="nav-link" id="benefitslink" href="/careers/benefits/">
                                Benefits
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item TopNavL">
                            <a class="nav-link" id="opportunitieslink" href="/careers/opportunities/">
                                Opportunities
                                <span class="sr-only">(current)</span>
                            </a>
                        </li>
                    </ul>
                </nav>


            </div>
        </nav>

