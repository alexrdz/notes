        <div class="off-canvas-nav fixed h-100 w-75 bg-white shadow-md">
            <button type="button" class="close close-button text-5xl font-thin mr-4 d-block js-close-nav-trigger" id="detail-modal-close-button" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">×</span>
            </button>
        </div>

    </header>
    <div class="container-fluid p-0">
        <header class="no-print">
  

            <!-- FIRST NAVBAR (ALWAYS VISIBLE, BUT SIMPLIFIED ON TABLETS AND BELOW) &  HAMBURGER MODAL (ONLY VISIBLE ON TABLETS AND LARGER) -->

            @Html.Partial("first-navbar")
            @Html.Partial("hamburger-modal")

            @if (!Model.Content.HideSubNavigation)
            {
                <div class="row no-gutters second-navbar hidden-md-down no-print">
                    <div class="col">
                        @Html.Partial("second-navbar")
                    </div>
                </div>
            }
        </header>

        <div class="row mb-3 no-gutters">
            <div class="col">
                <hr class="mb-5">
            </div>
        </div>

    </div>

    <!-- SEARCH MODAL (ONLY VISIBLE ON TABLETS AND SMALLER) -->
    <div class="container">
        <div class="row no-gutters">
            <div class="col">
                @Html.Partial("search-modal")
            </div>
        </div>
    </div>

    <!-- No JavaScript Warning. Only appears if user's browser does not support JavaScript, or if it is disabled -->
    <noscript>
        <div class="container">
            <div class="h4 m-3 text-center" style="color: rgb(255, 0, 0);">
                It appears that your JavaScript is disabled. Please enable JavaScript in your browser settings and reload this page.
            </div>
        </div>
    </noscript>

    <!-- MAIN CONTENT AREA + BREADCRUMB -->
    <main class="container-fluid mx-auto" id="main-content">

        @RenderBody()
        @RenderSection("bodyContent", false)

        <!-- RETURN TO TOP FEATURE -->
        <a href="#" class="return-to-top"><i class="fa fa-chevron-circle-up" aria-hidden="true"></i></a>

        <!-- CONTACT US MODAL -->
        <div id="contact-us-modal" class="modal fade right" role="dialog" tabindex="-1" aria-labelledby="contact-us-modal" data-backdrop="true"
             style="display: none;" aria-hidden="true">
            <div class="modal-dialog modal-full-height modal-right" id="hamburger-contact-dialog" aria-labelledby="hamburger-contact-dialog">
                <div class="modal-content"></div>
            </div>
        </div>

    </main>

    <!-- FOOTER SECTION -->
    <div class="container-fluid p-0">
        <footer class="container-fluid p-0 page-footer text-center bg-color-heisenberg mt-1">
            <hr>
            <div class="footer-content">
                @Html.Partial("widgets", siteSettings.FooterWidgets)
            </div>
        </footer>
    </div>

    @Html.RenderJsHere()
    <noscript>
        <style>
            body {
                visibility: visible;
            }
        </style>
    </noscript> <!-- A SAFEGUARD TO ENSURE THAT OUR BODY CONTENT IS VISIBLE EVEN IF JAVASCRIPT IS DISABLED -->
</body>

</html>